{
  "name": "react-redux-thunk-store",
  "version": "0.0.8",
  "description": "The component that help to simplify the way to create the redux store automatically.",
  "main": "lib/index.js",
  "scripts": {
    "all": "npm run build && npm run test:ci",
    "build": "npm run clean && npm run build:lib && npm run test-bundle",
    "build:lib": "babel src --out-dir lib --ignore tests",
    "clean": "rimraf {lib,dist,tmp}/*",
    "git:jest": "git submodule update --init --remote configs/jest",
    "test-bundle": "bundlesize",
    "test": "npm-run-all git:jest && jest --watch --coverage --config=configs/jest/jest.json",
    "test:ci": "npm-run-all git:jest && jest --ci --coverage --config=configs/jest/jest.json && codecov -t 34248b68-2f12-4523-8a32-c2dfa25ec239",
    "update": "ncu -u -a"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baoduy/react-redux-thunk-store.git"
  },
  "keywords": [
    "react",
    "redux",
    "thunk",
    "store"
  ],
  "author": "Duy Hoang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baoduy/react-redux-thunk-store/issues"
  },
  "homepage": "https://github.com/baoduy/react-redux-thunk-store#readme",
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "react": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-json-strings": "latest",
    "@babel/plugin-syntax-dynamic-import": "latest",
    "@babel/plugin-syntax-import-meta": "latest",
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "3.1.1",
    "bundlesize": "^0.17.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "jest": "^23.6.0",
    "jsdom-global": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.6.2",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.7.0",
    "redux-devtools-extension": "^2.13.7",
    "redux-immutable-state-invariant": "^2.1.0"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/tests/__coverage__/",
    "collectCoverage": true
  },
  "bundlesize": [
    {
      "path": "./lib/*.js",
      "compression": "gzip",
      "maxSize": "1 kB"
    }
  ]
}
